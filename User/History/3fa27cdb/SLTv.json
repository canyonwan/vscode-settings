{
  "name": "law-enforcement-web",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "test:unit": "vitest --environment jsdom",
    "coverage": "vitest run --coverage",
    "buildNoEmit": "vue-tsc --noEmit && vite build",
    "build": "vite build && esno ./build/script/postBuild.ts",
    "report": "cross-env REPORT=true npm run build",
    "preview": "vite preview",
    "fix-memory-limit": "cross-env lIMIT=4096 increase-memory-limit",
    "lint:es": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier:src": "prettier --write --loglevel warn \"src/**/*.{ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:prettier:all": "prettier --write .",
    "lint:style": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/"
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@amap/amap-jsapi-types": "^0.0.8",
    "@vicons/antd": "^0.12.0",
    "@vicons/carbon": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/material": "^0.12.0",
    "@vueuse/core": "^8.9.4",
    "axios": "^0.27.2",
    "d3": "^7.6.1",
    "date-fns": "^2.28.0",
    "echarts": "^5.3.3",
    "js-md5": "^0.7.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.31.0",
    "pdfjs-dist": "^2.14.305",
    "pinia": "^2.0.16",
    "qs": "^6.11.0",
    "swiper": "^8.3.1",
    "vfonts": "^0.1.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2",
    "vue-types": "^4.1.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.15",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.2",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.2.0",
    "esno": "^0.16.3",
    "jsdom": "^20.0.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "rollup-plugin-visualizer": "^5.7.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.9.3",
    "unplugin-vue-components": "^0.21.1",
    "vite": "^2.9.14",
    "vite-plugin-compression": "^0.3.6",
    "vite-plugin-html": "^2.1.2",
    "vitest": "^0.18.1",
    "vue-eslint-parser": "^9.0.3",
    "vue-tsc": "^0.38.9"
  }
}
