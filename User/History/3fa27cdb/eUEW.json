{
  "name": "law-enforcement-web",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "test:unit": "vitest --enviroment jsdom",
    "coverage": "vitest run --coverage",
    "buildNoEmit": "vue-tsc --noEmit && vite build",
    "build": "vite build && esno ./build/script/postBuild.ts",
    "report": "cross-env REPORT=true npm run build",
    "preview": "vite preview",
    "lint:es": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier:src": "prettier --write --loglevel warn \"src/**/*.{ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:prettier:all": "prettier --write .",
    "lint:style": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/"
  },
  "dependencies": {
    "@vicons/antd": "^0.11.0",
    "@vicons/carbon": "^0.12.0",
    "@vicons/ionicons5": "^0.11.0",
    "@vueuse/core": "^8.2.6",
    "axios": "^0.24.0",
    "d3": "^5.16.0",
    "date-fns": "^2.28.0",
    "echarts": "^5.3.2",
    "js-md5": "^0.7.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.28.2",
    "pdfjs-dist": "^2.13.216",
    "pinia": "^2.0.13",
    "qs": "^6.10.3",
    "swiper": "^8.1.1",
    "vfonts": "^0.1.0",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14",
    "vue-types": "^4.1.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.181",
    "@types/node": "^16.11.27",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-rc.21",
    "autoprefixer": "^10.4.4",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "esno": "^0.14.1",
    "jsdom": "^19.0.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "stylelint": "^14.7.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^3.21.0",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.6.3",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-vue-components": "^0.17.21",
    "vite": "^2.9.5",
    "vite-plugin-compression": "^0.3.6",
    "vite-plugin-html": "^2.1.2",
    "vitest": "^0.12.0",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^0.3.0"
  }
}
